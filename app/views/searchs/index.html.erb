<div class="container">

        <!-- Page Header -->
        <div class="row">
            <div class="col-lg-12">
                <% if @tags.empty? %>
                <h1 align="center" class="page-header">Results not found for <%= @search %></h1>
                <% else %>
                <h1 align="center" class="page-header">Search Results for <%= @search %></h1>
            </div>
        </div>
        <!-- /.row -->

        <!-- Projects Row -->
        <% @tags.each do |tag| %>
        <% tag.pictures.each do |picture| %>
          <div class="col-md-4 portfolio-item">
          </div>
            <div class=<%="row"-%>>
                <div class="col-md-4 portfolio-item">
                    <a href="#">
                        <%= image_tag picture.image.url(:medium), class:"img-responsive" %>
                        <% if logged_in? %>
                        <%= button_to "details", picture_path(picture), method: :get, class:"btn btn-primary" %>
                        <% else %>
                        <div> </div>
                        <% end %>
                    </a>
                    <h3>
                    </h3>
                    <p></p>
                </div>
            </div>
          <% end %>
      <% end %>
    <% end %>
